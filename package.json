{
  "name": "yaml-extra",
  "version": "1.0.0",
  "description": "yaml-extra contains methods that aren't included in the yaml package. Such create, read, write, copy, and remove.",
  "engines": {
    "node": ">=14.14"
  },
  "homepage": "https://github.com/iamando/yaml-extra",
  "repository": {
    "type": "git",
    "url": "https://github.com/iamando/yaml-extra"
  },
  "keywords": [
    "yaml",
    "file",
    "write",
    "copy",
    "extra",
    "read",
    "delete",
    "remove",
    "touch",
    "create",
    "output",
    "move",
    "promise",
    "dump",
    "load"
  ],
  "author": "iamando <iamando@tuta.io>",
  "license": "MIT",
  "main": "./lib/index.js",
  "exports": {
    ".": "./lib/index.js"
  },
  "files": [
    "lib/",
    "docs/"
  ],
  "sideEffects": false,
  "scripts": {
    "lint": "eslint \"./**/*.{js,ts}\"",
    "format": "prettier --write \"./**/*.{js,ts}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "standard:check": "standard",
    "standard:fix": "standard --fix",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "eslint": "^8.30.0",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "2.8.1",
    "standard": "^17.0.0",
    "validate-branch-name": "^1.3.0"
  },
  "dependencies": {
    "execa": "^7.1.0",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.1.0",
    "js-yaml": "^3.14.1",
    "node-yaml": "^4.0.1",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "universalify": "^2.0.0",
    "util": "^0.12.5",
    "write-yaml-file": "^4.2.0"
  }
}
