{
  "name": "yaml-extra",
  "version": "1.0.0",
  "description": "yaml-extra contains methods that aren't included in the yaml package. Such read, write",
  "engines": {
    "node": ">=14.14"
  },
  "sideEffects": false,
  "license": "MIT",
  "main": "./lib/index.js",
  "exports": {
    ".": "./lib/index.js"
  },
  "author": {
    "name": "iamando",
    "email": "iamando@tuta.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iamando/yaml-extra.git"
  },
  "bugs": {
    "url": "https://github.com/iamando/yaml-extra/issues"
  },
  "homepage": "https://github.com/iamando/yaml-extra#readme",
  "files": [
    "lib",
    "docs"
  ],
  "keywords": [
    "yaml",
    "file",
    "write",
    "extra",
    "read",
    "output",
    "promise",
    "dump"
  ],
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint \"./**/*.{js,ts}\"",
    "format": "prettier --write \"./**/*.{js,ts}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "standard:check": "standard",
    "standard:fix": "standard --fix",
    "release": "npm publish --follow-tags",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "eslint": "^8.30.0",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "2.8.7",
    "standard": "^17.0.0",
    "validate-branch-name": "^1.3.0"
  },
  "dependencies": {
    "execa": "^5.0.0",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.1.0",
    "js-yaml": "^3.14.1",
    "node-yaml": "^4.0.1",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "universalify": "^2.0.0",
    "util": "^0.12.5",
    "write-yaml-file": "^4.2.0"
  }
}
