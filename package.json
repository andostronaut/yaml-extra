{
  "name": "yaml-extra",
  "version": "1.0.0",
  "description": "yaml-extra contains methods that aren't included in the yaml package. Such load, dump, create, read, write, copy, and remove.",
  "engines": {
    "node": ">=14.14"
  },
  "homepage": "https://github.com/iamando/yaml-extra",
  "repository": {
    "type": "git",
    "url": "https://github.com/iamando/yaml-extra"
  },
  "keywords": [
    "yaml",
    "file",
    "write",
    "copy",
    "extra",
    "read",
    "delete",
    "remove",
    "touch",
    "create",
    "output",
    "move",
    "promise",
    "dump",
    "load"
  ],
  "author": "iamando <iamando@tuta.io>",
  "license": "MIT",
  "main": "./lib/index.js",
  "exports": {
    ".": "./lib/index.js"
  },
  "files": [
    "lib/",
    "docs/"
  ],
  "sideEffects": false,
  "scripts": {
    "lint": "standard",
    "eslint": "eslint \"./**/*.{js,ts}\"",
    "format": "prettier --write \"./**/*.{js,ts}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "eslint": "^8.30.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.0",
    "prettier": "2.8.1",
    "standard": "^17.0.0",
    "validate-branch-name": "^1.3.0"
  }
}
